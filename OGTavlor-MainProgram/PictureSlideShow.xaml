<Window x:Class="OGTavlor_MainProgram.PictureSlideShow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OGTavlor_MainProgram"
        mc:Ignorable="d"
        Loaded="PictureSlideShow_Loaded"
        Title="PictureSlideShow" Height="500" Width="700">
    <Grid>
        <Image x:Name="ImgSlideShow" HorizontalAlignment="Left" Height="389" Margin="10,10,0,0" VerticalAlignment="Top" Width="672"/>
        <Button x:Name="BtnPrevious" Content="Föregående" HorizontalAlignment="Left" Margin="60,417,0,0" VerticalAlignment="Top" Width="75" Click="PreviousPicture"/>
        <Button x:Name="BtnMainWindow" Content="Startsida" HorizontalAlignment="Left" Margin="315,417,0,0" VerticalAlignment="Top" Width="75" Click="btnMainWindow_Click"/>
        <Button x:Name="BtnNext" Content="Nästa" HorizontalAlignment="Left" Margin="576,417,0,0" VerticalAlignment="Top" Width="75" Click="NextPicture"/>
        <Button x:Name="StopSlide" Content="Stoppa bildspelet" HorizontalAlignment="Left" Margin="352,442,0,0" VerticalAlignment="Top" Width="110" Click="StopSlideShow"/>
        <Button x:Name="StartSlide" Content="Starta bildspelet" HorizontalAlignment="Left" Margin="245,442,0,0" VerticalAlignment="Top" Width="102" Click="StartSlideShow"/>
    </Grid>
</Window>
